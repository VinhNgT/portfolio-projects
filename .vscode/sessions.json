{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
  "theme": "tribe",
  "active": "driving_license",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "sessions": {
    "driving_license": [
      {
        "name": "driving_license build_runner watch -d",
        "autoExecuteCommands": true,
        "joinOperator": ";",
        "commands": [
          "cd .\\apps\\driving_license\\",
          "dart run build_runner watch -d"
        ]
      },
      {
        "name": "driving_license extra terminal",
        "autoExecuteCommands": true,
        "joinOperator": ";",
        "commands": [
          "cd .\\apps\\driving_license\\"
        ]
      }
    ]
  }
}